{
  "private": true,
  "name": "getto-project-example-ui",
  "version": "1.3.0-ui",
  "scripts": {
    "start:public": "webpack serve --mode development --config ui/public/webpack.config.js",
    "start:secure": "webpack serve --mode development --config ui/secure/webpack.config.js",
    "build": "npm run setup && npm run webpack",
    "webpack": "npm run webpack:public && npm run webpack:secure",
    "webpack:public": "webpack --mode production --config ui/public/webpack.config.js",
    "webpack:secure": "webpack --mode production --config ui/secure/webpack.config.js",
    "setup": "npm run setup:html && npm run setup:protobuf && npm run setup:css && npm run setup:environment",
    "setup:html": "node ui/setup/generate-html.js",
    "setup:protobuf": "sh ui/setup/protobuf.sh",
    "setup:environment": "node ui/setup/environment.js",
    "setup:css": "node ui/setup/css.js",
    "test": "npm run setup && npm run test:secretlint && npm run test:eslint && npm run test:vitest",
    "test:eslint": "eslint src/",
    "test:secretlint": "git ls-files | xargs secretlint",
    "test:vitest": "vitest run --config ui/setup/config/vitest.config.ts"
  },
  "author": "shun@getto.systems",
  "license": "MIT",
  "devDependencies": {
    "@prisma/client": "^4",
    "@secretlint/secretlint-rule-preset-recommend": "^5",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "@vitest/coverage-c8": "^0.26.2",
    "autoprefixer": "^10",
    "c8": "^7",
    "clean-css": "^5",
    "eslint": "^8",
    "eslint-plugin-jest": "^27",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "htm": "^3",
    "jest": "^29",
    "postcss": "^8",
    "postcss-cli": "^10",
    "preact": "^10",
    "prisma": "^4",
    "protobufjs": "^7",
    "protobufjs-cli": "^1",
    "secretlint": "^5",
    "terser-webpack-plugin": "^5",
    "ts-loader": "^9",
    "typescript": "^4.9.4",
    "vitest": "^0",
    "webpack": "^5",
    "webpack-cli": "^5",
    "webpack-dev-server": "^4",
    "worker-plugin": "^5"
  }
}
