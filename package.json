{
  "private": true,
  "name": "getto-project-example-ui",
  "version": "0.20.4-ui",
  "scripts": {
    "storybook": "cd storybook && npm run build",
    "start:public": "webpack serve --mode development --config ui/public/webpack.config.js",
    "start:secure": "webpack serve --mode development --config ui/secure/webpack.config.js",
    "build": "npm run setup && npm run webpack",
    "webpack": "npm run webpack:public && npm run webpack:secure",
    "webpack:public": "webpack --mode production --config ui/public/webpack.config.js",
    "webpack:secure": "webpack --mode production --config ui/secure/webpack.config.js",
    "setup": "npm run setup:html && npm run setup:protobuf && npm run setup:theme && npm run setup:css && npm run setup:environment",
    "setup:html": "node ui/setup/generate-html.js",
    "setup:protobuf": "sh ui/setup/protobuf.sh",
    "setup:theme": "sh ui/setup/theme.sh",
    "setup:environment": "node ui/setup/environment.js",
    "setup:css": "node ui/setup/css.js",
    "test": "npm run setup && npm run test:eslint && npm run test:jest",
    "test:eslint": "eslint src/ ui/vendor",
    "test:jest": "jest -c ui/config/jest.config.js"
  },
  "author": "shun@getto.systems",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.18.1",
    "@typescript-eslint/parser": "^4.31.1",
    "autoprefixer": "^10.2.5",
    "clean-css": "^5.1.2",
    "eslint": "^7.22.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "htm": "^3.0.4",
    "jest": "^26.6.3",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "preact": "^10.5.14",
    "prisma": "^2.26.0",
    "protobufjs": "^6.11.2",
    "terser-webpack-plugin": "^5.1.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "typescript": "^4.4.2",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "worker-plugin": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^2.26.0"
  }
}
