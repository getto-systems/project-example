export APP_ROOT=$(pwd)
export PUBLIC_PORT=80
export SECURE_PORT=88
set-window-title $(basename $APP_ROOT):${PUBLIC_PORT}/${SECURE_PORT}

export DOCKER_WRAPPER_RC=$APP_ROOT/.docker-wrapper.sh

export PUBLIC_APP_PORT=10680
export SECURE_APP_PORT=10688

# api
export PORT=10690
export DOMAIN=$USER.dev.getto.systems
export UI_HOST=dev-example-ui.$DOMAIN
export ORIGIN=https://$UI_HOST
export CLOUDFRONT_RESOURCE='https://secure.example.getto.systems/*'

export SECRET_TICKET_PRIVATE_KEY=$(cat ./.secret/ec384-key-pair.pkcs8.pem)
export SECRET_TICKET_PUBLIC_KEY=$(cat ./.secret/ec384-key-pair.pub.pem)

export SECRET_API_PRIVATE_KEY=$(cat ./.secret/ec384-key-pair.pkcs8.pem)
export SECRET_API_PUBLIC_KEY=$(cat ./.secret/ec384-key-pair.pub.pem)

export SECRET_RESET_TOKEN_PRIVATE_KEY=$(cat ./.secret/ec384-key-pair.pkcs8.pem)
export SECRET_RESET_TOKEN_PUBLIC_KEY=$(cat ./.secret/ec384-key-pair.pub.pem)

export SECRET_CLOUDFRONT_PRIVATE_KEY=$(cat ./.secret/cloudfront-pk.pem)
